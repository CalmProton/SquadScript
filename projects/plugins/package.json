{
  "name": "@squadscript/plugins",
  "version": "0.1.0",
  "description": "Official plugin collection for SquadScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target bun && bun run build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "check-types": "tsc --noEmit",
    "dev": "bun --watch src/index.ts",
    "clean": "rm -rf dist",
    "test": "bun test"
  },
  "keywords": [
    "squad",
    "squadjs",
    "squadscript",
    "plugins"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@socket.io/bun-engine": "^0.1.0",
    "@squadscript/core": "workspace:*",
    "@squadscript/types": "workspace:*",
    "elysia": "^1.4.22",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "typescript": "^5.9.2"
  }
}
